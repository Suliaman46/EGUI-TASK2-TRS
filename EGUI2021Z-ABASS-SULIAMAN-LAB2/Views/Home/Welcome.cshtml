<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>


<form action="https://localhost:7256/Home/Welcome" method="GET">
  <div>
    <label for="say">Please Select Date : </label>
    <input name="dateString" id="dateString" value="yyyy-mm-dd">
  </div>
  <div>
    <button>Show Table</button>
  </div>
</form>

@*<div>
    @Html.Partial("~/Views/Shared/EditorTemplates/_Date.cshtml")
</div>*@

<table class="table table-bordered table-responsive table-hover">  
    <tr>  
        <th>Date </th>  
        <th>Code</th>  
        <th>Time</th>  
        <th>Description</th>  
    </tr>  
    @foreach (var d in Model)  
    {  
        <tr>  
            <td>@d.date</td>  
            <td>@d.code</td>  
            <td>@d.time</td>  
            <td>@d.description</td>  
 
  
        </tr>  
    }  
</table>